    "### Variables\n",
    "\n",
    "- $\n",
    "x^{i, j}_{k, t} =\n",
    "\\begin{cases}\n",
    "    1 & \\text{si el profesor } i\n",
    "    \\text{ tiene asignada la materia } j\n",
    "    \\text{ en el aula } k\n",
    "    \\text{ en el horario } t \\\\\n",
    "    0 & \\text{si no}\n",
    "\\end{cases}\n",
    "$\n",
    "\n",
    "- $\n",
    "y_{i, j} =\n",
    "\\begin{cases}\n",
    "    1 & \\text{si el profesor } i\n",
    "    \\text{ puede enseñar la materia } j \\\\\n",
    "    0 & \\text{si no}\n",
    "\\end{cases}\n",
    "$\n",
    "\n",
    "- $\n",
    "\\hat{y}_{i, j} =\n",
    "\\begin{cases}\n",
    "    1 & \\text{si el profesor } i\n",
    "    \\text{ tiene asignada la materia } j \\\\\n",
    "    0 & \\text{si no}\n",
    "\\end{cases}\n",
    "$\n",
    "\n",
    "- $\n",
    "z_{k, j} =\n",
    "\\begin{cases}\n",
    "    1 & \\text{si el aula } k\n",
    "    \\text{ esta asignada a la materia } j \\\\\n",
    "    0 & \\text{si no}\n",
    "\\end{cases}\n",
    "$\n",
    "\n",
    "- $\n",
    "\\hat{z}_{j, d} =\n",
    "\\begin{cases}\n",
    "    1 & \\text{si la materia } j\n",
    "    \\text{ se imparte el dia } d \\\\\n",
    "    0 & \\text{si no}\n",
    "\\end{cases}\n",
    "$\n",
    "\n",
    "- $\n",
    "s_{j, t, d} =\n",
    "\\begin{cases}\n",
    "    1 & \\text{si el curso } j\n",
    "    \\text{ comienza a la hora } t\n",
    "    \\text{ en el dia } d \\\\\n",
    "    0 & \\text{si no}\n",
    "\\end{cases}\n",
    "$"
   ]
  },
  {
