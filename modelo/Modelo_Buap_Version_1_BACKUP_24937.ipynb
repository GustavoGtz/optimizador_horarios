    "if hasattr(model, 'Restriccion_Duracion_Sesion'):\n",
    "    model.del_component(model.Restriccion_Duracion_Sesion)\n",
    "\n",
    "def X_jd_init(model, j, d):\n",
    "    return [\n",
    "        x for x in model.X\n",
    "        if x[MATERIA] == j and x[DIA] == d\n",
    "    ]\n",
    "\n",
    "model.X_jd = pyo.Set(within=model.X, initialize=X_jd_init(model))\n",
    "\n",
    "def duracion_sesion_materia_rule(model, j, d):\n",
    "    return (model.Psi_Min[j] * model.Z_hat[j, d],\n",
    "            sum(model.X[x] for x in model.X_jd[(j, d)]),\n",
    "            model.Psi_Min[j] * model.Z_hat[j, d])\n",
    "\n",
    "model.Restriccion_Duracion_Sesion = pyo.Constraint(model.Materias, model.Dias, rule=duracion_sesion_materia_rule)"
   ]
